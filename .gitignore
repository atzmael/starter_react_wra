# dependencies
/node_modules
/.pnp
.pnp.js

# testing
/coverage

# production
/build

# misc
.DS_Store
.env.local
.env.development.local
.env.test.local
.env.production.local

npm-debug.log*
yarn-debug.log*
yarn-error.log*

# ignore everything in the root except the "wp-content" directory.
admin/!wp-content/

# ignore everything in the "wp-content" directory, except:
# "mu-plugins", "plugins", "themes" directory
admin/wp-content/*
admin/!wp-content/mu-plugins/
admin/!wp-content/plugins/
admin/!wp-content/themes/

# ignore these plugins
admin/wp-content/plugins/hello.php

# ignore node dependency directories
admin/node_modules/

# ignore log files and databases
admin/*.log
admin/*.sql
admin/*.sqlite